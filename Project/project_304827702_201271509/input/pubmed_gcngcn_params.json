{
  "dataset": "PubMed",
  "split": "random",
  "seed": 28,
  "save_model": false,
  "cuda": 3,
  "epochs": 400,
  "eval_every": 1,
  "print_Cs": true,
  "load_pretrained_model": false,
  "lr": 0.005,
  "weight_decay": 5e-4,
  "decay_every": 400,
  "decay_factor": 0.96,

  "p_input": 0.6,
  "use_hypernetworks": false,
  "p_hyper": 0.1,
  "f_n_hidden": 4,
  "f_hidden_size": 128,
  "c_dict": {
    "out": {
      "requires_grad": false,
      "init_val": 0.5,
      "update_every": 1
    },
    "hyper": {
      "requires_grad": false,
      "init_val": 0.5,
      "update_every": 1
    }
  },
  "models_dict": {

    "model1": {
      "name": "gcn",
      "params1": {
        "out_channels": 64,
        "cached": true,
        "bias": true,
        "normalize": true
      },
      "hyperparams1": {
       "lr": 0.01,
        "weight_decay": 5e-4
      },
      "params2": {
        "cached": true,
        "bias": false,
        "normalize": true
      },
      "hyperparams2": {
        "lr": 0.01,
        "weight_decay": 0.0
      }
    },
    "model2": {
      "name": "gcn",
      "params1": {
        "out_channels": 64,
        "cached": true,
        "bias": true,
        "normalize": true
      },
      "hyperparams1": {
        "lr": 0.01,
        "weight_decay": 5e-4
      },
      "params2": {
        "cached": true,
        "bias": false,
        "normalize": true
      },
      "hyperparams2": {
        "lr": 0.01,
        "weight_decay": 0.0
      }
    }
  },
  "hypernetworks_dict": {
    "lr": 1e-3,
    "weight_decay": 5e-3
  }
}